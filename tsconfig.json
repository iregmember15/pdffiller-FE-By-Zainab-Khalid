{
  "compilerOptions": {
    "target": "es6",
    //"module": "esnext",
    "module": "commonjs",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "strict": true,
    "jsx": "react-jsx",
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "outDir": "./dist",
    "allowJs": false
  },
  "include": ["src/**/*", "tests/**/*"]
}
